# GBox CUA Agent Environment Variables
# Copy this file to .env and fill in your actual values

# =============================================================================
# GBox API Configuration (Required)
# =============================================================================
# GBox API Key - Get from https://gbox.ai
GBOX_API_KEY=gbox_xxxxxxxxxxxxxxxxxxxxx

# =============================================================================
# VLM Provider Configuration
# =============================================================================
# VLM Provider: "vllm", "openai", or "openrouter"
VLM_PROVIDER=openai

# =============================================================================
# OpenAI Configuration (Required if VLM_PROVIDER=openai)
# =============================================================================
OPENAI_API_KEY=sk-xxxxxxxxxxxxxxxxxxxxx
OPENAI_API_BASE=https://api.openai.com/v1

# =============================================================================
# OpenRouter Configuration (Required if VLM_PROVIDER=openrouter)
# =============================================================================
OPENROUTER_API_KEY=sk-or-xxxxxxxxxxxxxxxxxxxxx
OPENROUTER_API_BASE=https://openrouter.ai/api/v1

# =============================================================================
# vLLM Configuration (Required if VLM_PROVIDER=vllm)
# =============================================================================
# vLLM API Base URL (e.g., http://localhost:8000/v1)
VLLM_API_BASE=http://localhost:8000/v1
VLLM_API_KEY=EMPTY

# =============================================================================
# Model Configuration
# =============================================================================
# Model name (default: unsloth/Qwen3-VL-30B-A3B-Instruct for rl-cua, gpt-4o for OSWorld)
MODEL_NAME=gpt-4o

# Maximum tokens to generate (default: 2048)
MAX_TOKENS=2048

# Temperature for sampling (default: 0.7)
TEMPERATURE=0.7

# Top-p sampling (default: 0.9)
TOP_P=0.9

# Maximum turns/steps (default: 20)
MAX_TURNS=20

# =============================================================================
# Box Configuration
# =============================================================================
# Box type: "android" or "linux" (default: android)
BOX_TYPE=android

# =============================================================================
# Task Configuration (for standalone usage)
# =============================================================================
# Task description (can also be passed as command line argument)
# Note: If the task contains spaces, make sure to quote it in .env file:
# TASK="Open the Settings app"
TASK="Open the Settings app"

# =============================================================================
# Optional Configuration
# =============================================================================
# Log level: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=INFO

# Verbose output (true/false)
VERBOSE=false

